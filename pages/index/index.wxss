/**index.wxss**/
page{
  height: 100%;
  width: 100%;
  background: #f8f8f8;
}
.box{
  width:100%;
  height:100%;
  background: #f8f8f8;
  
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.title{
  width: 100%;
}
.item{
  padding: 5px;
  margin: 5px 15px;
  background: white;
  border: 1px solid #ccc;
  border-radius: 10px;

  display: flex;
  flex-direction: column;
  justify-content: center;
}