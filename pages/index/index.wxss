/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 50px;
}
.Cwindow{
  color:red;
}
.Dwindow{
  color:yellow;
}

.body{
  text-align: center;
}
.title{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.item{
  width: 90%;
  height: 60px;
  margin: 5px;
  text-align: left;
  background: #f8f8f8;
  display: block;
  padding: 10px 10px;
  border:1px solid #ccc;
  border-radius: 10px;
}